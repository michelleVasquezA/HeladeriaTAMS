@page
@model RegisterModel
@{
    ViewData["Title"] = "USUARIO";
    ViewData["Title1"] = "Nuevo Usuario";
}

 

<h1 style="text-align: left;color: #B22902;">@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4" style="width: 450px;padding: 10px 20px;margin: auto;background-color: white;box-sizing: border-box;border-radius: 20px;background-attachment: fixed;border: 3px solid #FFBCA7">
        <form id="registerForm" asp-route-returnUrl="@Model.ReturnUrl" method="post">
    <div class="form-group"  align="center">
            
            <hr />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <h4 style="text-align:center;font-family: Monaco;"><b>@ViewData["Title1"]</b></h4>
            <br>
            <div class="form-floating">
                <input asp-for="Input.Email" class="form-control" autocomplete="username" aria-required="true" />
                <label asp-for="Input.Email"></label>
                <span asp-validation-for="Input.Email" class="text-danger"></span>
            </div>
            <br>
            <div class="form-floating">
                <input asp-for="Input.Password" class="form-control" autocomplete="new-password" aria-required="true" />
                <label asp-for="Input.Password"></label>
                <span asp-validation-for="Input.Password" class="text-danger"></span>
            </div>
            <br>
            <div class="form-floating">
                <input asp-for="Input.ConfirmPassword" class="form-control" autocomplete="new-password" aria-required="true" />
                <label asp-for="Input.ConfirmPassword"></label>
                <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
            </div>
            <br>
            <button id="registerSubmit" type="submit" class="w-100 btn btn-lg btn-primary">Register</button>
        </form>
    </div>
